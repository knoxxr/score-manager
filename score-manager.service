[Unit]
Description=Score Manager Web Application
After=network.target

[Service]
Type=simple
User=dashinaru
WorkingDirectory=/home/dashinaru/Git/score-manager
ExecStart=/home/dashinaru/Git/score-manager/start_server.sh
Restart=always
RestartSec=10
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=score-manager

[Install]
WantedBy=multi-user.target
